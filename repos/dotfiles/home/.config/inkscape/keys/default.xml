<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<keys
   name="My custom shortcuts">
  <bind
     key="v"
     action="ToolSelector"
     display="true" />
  <bind
     key="V"
     action="ToolSelector" />
  <bind
     key="b"
     action="ToolPen"
     display="true" />
  <bind
     key="B"
     action="ToolPen" />
  <bind
     key="c"
     action="ToolArc"
     display="true" />
  <bind
     key="C"
     action="ToolArc" />
  <bind
     key="l"
     action="org.inkscape.effect.eqtexsvg"
     display="true" />
  <bind
     key="L"
     action="org.inkscape.effect.eqtexsvg" />
  <bind
     key="f"
     action="EditSelectSameFillStroke"
     display="true" />
  <bind
     key="F"
     action="EditSelectSameFillStroke" />
  <bind
     key="o"
     action="org.inkscape.effect.eqtexsvg.noprefs"
     display="true" />
  <bind
     key="O"
     action="org.inkscape.effect.eqtexsvg.noprefs" />
</keys>
