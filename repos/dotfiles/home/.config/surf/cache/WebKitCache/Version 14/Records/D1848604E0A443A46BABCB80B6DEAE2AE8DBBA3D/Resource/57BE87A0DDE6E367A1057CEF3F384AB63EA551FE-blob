  <semantics>
    <mrow class="MJX-TeXAtom-ORD">
      <mstyle displaystyle="true" scriptlevel="0">
        <mi>f</mi>
        <mo>&#x003A;<!-- : --></mo>
        <mi>A</mi>
        <mo stretchy="false">&#x2192;<!-- → --></mo>
        <mi>Y</mi>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle f\colon A\to Y}</annotation>
  </semantics>
