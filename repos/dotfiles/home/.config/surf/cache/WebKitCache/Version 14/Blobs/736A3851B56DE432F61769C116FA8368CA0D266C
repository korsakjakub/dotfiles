<script>
  var globalParams = {
    cacheTime: 15000
  }

  // select the insurance and scroll to top
  $(function () {
    var $insInput = $('#tab-nav-insurance');
    var inSelectIns = false;

    $('body').on('click', function (e) {
      var $target = $(e.target)

      if (inSelectIns && $('.tab-nav.nav-fixed').length) {
        setTimeout(function () {
          !$insInput.prop('checked') && $('html, body').animate({
            scrollTop: $('#tab-nav-control').offset().top
          });
        }, 50)
      }

      setTimeout(function () {
        inSelectIns = $insInput.prop('checked')
      }, 50)
    })
  });

  
</script>

<script>
  if (window.emarsysButton) {
    var jsonText = document.getElementById("data-device").innerHTML;
    try {
      var jsonPhoneObj = JSON.parse(jsonText);
      var productSku = jsonPhoneObj.sku;
      ScarabQueue.push(['view', productSku]);
    } catch (e) {
    }
    ScarabQueue.push(['category', 'Phone']);
  }
</script>
