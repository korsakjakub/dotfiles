<script type="text/javascript">
   function addOptionPrice(that)
   {
   		var name = "price" + that.name;
   		var price = $(that).data("price");
   		var webpriceid = "web" + name;
   		var webprice = $(that).data("webprice");
   		var titleid = "name" + that.name;
   		var title = $(that).data("title");

   		if(that.checked) {
   			var inputStr = '<input id="' + name + '" type="hidden" name="' + name + '" value="' + price + '"/>'
   			$(that).after(inputStr);
   			var inputStr = '<input id="' + webpriceid + '" type="hidden" name="' + webpriceid + '" value="' + webprice + '"/>'
   			$(that).after(inputStr);
   			var inputStr = '<input id="' + titleid + '" type="hidden" name="' + titleid + '" value="' + title + '"/>'
   			$(that).after(inputStr);
   		} else {
   			$("#" + name).remove();
   			$("#" + webpriceid).remove();
   			$("#" + titleid).remove();
   		}
   }
</script><link rel="stylesheet" type="text/css" href="/_ui/desktop/common/css/promotedOption.css?v=20190523085236" />
